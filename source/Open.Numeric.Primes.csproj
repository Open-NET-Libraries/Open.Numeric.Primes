<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>electricessence</Authors>
    <Company />
    <Product />
    <Description>Methods and extensions for prime number detection and discovery.

Part of the "Open" set of libraries.</Description>
    <Copyright>https://github.com/electricessence/Open.Numeric.Primes/blob/master/LISCENSE.md</Copyright>
    <PackageLicenseUrl>https://github.com/electricessence/Open.Numeric.Primes/blob/master/LISCENSE.md</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/electricessence/Open.Numeric.Primes/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/electricessence/Open.Numeric.Primes/</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>prime-numbers, primes, prime-factorization, prime-detection, prime-discovery</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>1.4.6</Version>
    <AssemblyVersion>1.4.6.0</AssemblyVersion>
    <FileVersion>1.4.6.0</FileVersion>
    <PackageReleaseNotes>Casting fix for Int64 and some BigInteger to UInt64 conversions.
</PackageReleaseNotes>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <Optimize>True</Optimize>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
    <PackageReference Include="Open.Collections" Version="2.4.6" />
    <PackageReference Include="System.Memory" Version="4.5.2" />
  </ItemGroup>
</Project>